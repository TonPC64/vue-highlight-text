{
  "name": "vue-highlight-text",
  "description": "Vue component for highlight multiple istances of a word",
  "version": "1.6.2",
  "author": "Chanwit Piromplad <kingkong2103@gmail.com>",
  "license": "MIT",
  "main": "public/vue-highlight-text.min.js",
  "files": [
    "public"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TonPC64/vue-highlight-text.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:directive": "bili src/components/HighlightText.js --filename directive.js",
    "build:component": "bili src/components/VueHighlightText.vue --filename [name].js",
    "bundle": "npm run build:directive && npm run build:component"
  },
  "dependencies": {
    "to-style": "^1.3.3",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "bili": "^3.1.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "rollup-plugin-vue": "^4.3.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "begeta": "^1.0.0",
    "buefy": "^0.7.0",
    "highlight.js": "^9.13.1",
    "vue-embed": "^1.0.0",
    "vue-highlight-component": "^1.0.0"
  }
}
